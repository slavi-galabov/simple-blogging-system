<section class="row">
    {{#each articles}}
        <dic class="col-sm-6 ">
        <article class="article-list">

                <div class="col-sm-12 text-center">
                    <a href="/article/details/{{_id}}"><h2>{{title}}</h2></a>
                </div>
                <div class="col-sm-12 excerpt"> {{excerpt}}</div>
                <div class="col-sm-12  text-center">
                    <a href="/article/details/{{_id}}" class="read-more"><span>Read more </span></a>
                </div>

        </article>
        </dic>
    {{/each}}
</section>

<ul class="pagination">
    {{#each pages}}
        <li><a href="/article/list/{{this}}">{{this}}</a></li>
    {{/each}}
</ul>